    "react-i18next": "^11.18.6",
    "react-redux": "^7.2.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1"
  },
  "loki": {
    "looks-same": {
      "ignoreCaret": true,
      "strict": false
    },
    "chromeFlags": "--disable-font-subpixel-positioning --disable-lcd-text",
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
