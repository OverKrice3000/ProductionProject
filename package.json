{
  "name": "productionproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "dev:frontend": "webpack serve --env mode=development --env port=3000",
    "dev:server": "node ./json-server/index.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\" ",
    "typecheck": "tsc --noemit",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "prettier": "npx prettier --write \"**/*.{ts,tsx}\"",
    "prettier:check": "npx prettier --check \"**/*.{ts,tsx}\"",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "lint:ts": "eslint -c config/eslint/.eslintrc.js src config webpack.config.ts",
    "lint:ts:fix": "eslint -c config/eslint/.eslintrc.js --fix src config webpack.config.ts",
    "lint:scss": "stylelint --config config/stylelint/.stylelintrc.js \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint --config config/stylelint/.stylelintrc.js --fix \"src/**/*.scss\"",
    "test:unit": "jest --config=config/jest/jest.config.ts",
    "test:e2e": "cypress open --config-file cypress.config.ts",
    "test:ui:build-image": "docker build --platform linux/amd64 -f lokiDockerfile -t loki-chromium-amd .",
    "test:ui": "npm run storybook:build && npx loki --diffingEngine looks-same --chromeDockerImage loki-chromium-amd --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --diffingEngine looks-same --chromeDockerImage loki-chromium-amd  --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook:dev": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "prepare": "husky install && npm run test:ui:build-image",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "remove:feature": "ts-node ./scripts/features/removeFeature.ts",
    "postinstall": "rm -rf node_modules/.cache"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c config/eslint/.eslintrc.js",
      "prettier --list-different"
    ],
    "*.scss": [
      "stylelint --config config/stylelint/.stylelintrc.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.5",
