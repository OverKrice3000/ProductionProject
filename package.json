{
  "name": "productionproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "dev:frontend": "webpack serve --env mode=development --env port=3000",
    "dev:server": "node ./json-server/index.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\" ",
    "typecheck": "tsc --noemit",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
    "lint:js": "eslint -c config/eslint/.eslintrc.js src config webpack.config.ts",
    "lint:js:fix": "eslint -c config/eslint/.eslintrc.js --fix src config webpack.config.ts",
    "lint:scss": "stylelint --config config/stylelint/.stylelintrc.js \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint --config config/stylelint/.stylelintrc.js --fix \"src/**/*.scss\"",
    "test:unit": "jest --config=config/jest/jest.config.ts",
    "test:ui": "npm run storybook:build && npx loki --diffingEngine looks-same --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --diffingEngine looks-same --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "storybook:dev": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint -c config/eslint/.eslintrc.js"
    ],
    "*.scss": [
      "stylelint --config config/stylelint/.stylelintrc.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@jest/types": "^27.5.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "babel-loader": "^8.2.3",
    "concurrently": "^9.2.1",
    "css-loader": "^6.6.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^27.0.1",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "i18next-fs-backend": "^2.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "json-server": "^0.17.0",
    "lint-staged": "^16.2.7",
    "loki": "^0.35.1",
    "mini-css-extract-plugin": "^2.5.3",
    "react-refresh": "^0.12.0",
    "reg-cli": "^0.18.13",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "storybook-addon-themes": "^6.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.5.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^1.13.2",
    "i18next": "^23.2.7",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^4.0.10",
    "react-i18next": "^11.15.5",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1"
  },
  "loki": {
    "looks-same": {
      "ignoreCaret": true,
      "tolerance": 3,
      "strict": false
    },
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  }
}
